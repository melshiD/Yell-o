<% layout('layouts/boilerplate') %>
    <div class="row">
        <div class="col-6 offset-3">
            <div class="alert alert-danger" role="alert">
                <h4 class="alert-heading">Error: <%= err.statusCode %>
                </h4>
                <h5><%= err.message  %> </h5>
                <hr>
                <p class="mb-0">
                    <%= err.stack %>
                </p>
            </div>
        </div>
    </div>